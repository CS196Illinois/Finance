import pandas as pd

crimeData = pd.read_csv("/Users/ahad/Desktop/SacramentocrimeJanuary2006.csv")
print(crimeData)
for col in crimeData.columns:
    print(col)

crimeDistricts = crimeData[['district']]

#print(crimeDistricts)

mostDangerousDistrict = crimeDistricts.mode()

#print(mostDangerousDistrict)

allCrimeType = crimeData[['crimedescr']]

#print(allCrimeType)
#allCrimeType = allCrimeType.astype(str)

for crimedescr in allCrimeType:
    crimeType = allCrimeType[crimedescr].astype(str)
    if 'BURGLARY' in crimeType:
        print(crimeType)

homicides = crimeData[allCrimeType.apply(lambda row: row.astype(str).str.contains('HOMICIDE').any(), axis=1)]

addressOfHomicide = homicides[['address', 'cdatetime']]

print(addressOfHomicide)
